<table class="table table-bordered table-condensed table-striped table-hover">
    <thead>
    <tr>
        <th>Player</th>
        <th>Best time</th>
    </tr>
    </thead>
    <tbody>
    {% for result in entity.type.results %}
    <tr>
        <td>{{ result.object }}</td>
        <td>{{ result.time | gstime }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
